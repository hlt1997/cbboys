ACA,aAAA,MAAA,CAAA;AACA,UAAA,OAAA,EAAA,OADA;AAEA,UAAA,QAAA,EAAA;AAFA,SAAA;AAIA,aAAA,SAAA,GAAA,OAAA;AACA,eAAA,KAAA;AACA;AACA,KAtCA;AAuCA,IAAA,OAvCA,qBAuCA;AACA;AACA,UAAA,KAAA,WAAA,IAAA,EAAA,EAAA;AACA,YAAA,KAAA,QAAA,IAAA,KAAA,WAAA,EAAA;AACA,eAAA,MAAA,CAAA;AACA,YAAA,OAAA,EAAA,SADA;AAEA,YAAA,QAAA,EAAA;AAFA,WAAA;AAIA,iBAAA,KAAA;AACA,eAAA,YAAA,GAAA,OAAA;AAEA,SARA,MAQA;AACA,eAAA,YAAA,GAAA,SAAA;AACA,iBAAA,IAAA;AACA;AACA;AACA,KAvDA;AAwDA,IAAA,MAxDA,oBAwDA;AAAA;;AACA,UAAA,KAAA,OAAA,MAAA,KAAA,OAAA,EAAA,IAAA,KAAA,OAAA,EAAA,EAAA;AACA;AACA,aAAA,KAAA,CAAA,IAAA,CAAA,WAAA,EAAA,cAAA,KAAA,QAAA,GAAA,YAAA,GAAA,KAAA,QAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA;AACA;AACA,cAAA,GAAA,CAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA;AACA,YAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,WAHA,MAGA;AACA;AACA,YAAA,KAAA,CAAA,WAAA,CAAA,MAAA,EAAA,SAAA;AACA;AACA,SAVA;AAWA;AACA;AAvEA;AAXA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!--顶部开始-->\r\n    <mt-header title=\"账号注册\" class=\"header\"></mt-header>\r\n    <!--顶部结束-->\r\n    <div>\r\n      <img src=\"../../public/img/index/index_01.jpg\" alt=\"\">\r\n      <!-- 账户信息开始 -->\r\n      <mt-field \r\n        type=\"text\"\r\n        label=\"用户名：\"\r\n        placeholder=\"请输入用户名\"\r\n        :attr=\"{maxlength:20}\"\r\n        v-model=\"username\" \r\n        :state=\"NameStatus\"\r\n        @blur.native.capture=\"handle1\"> \r\n      </mt-field>\r\n      <mt-field \r\n        type=\"password\"\r\n        label=\"密码：\"\r\n        placeholder=\"请输入密码\"\r\n        :attr=\"{maxlength:20,autocomplete:'off'}\"\r\n        v-model=\"password\"\r\n        :state=\"pwdStatus\"\r\n        @blur.native.capture=\"handle2\"> \r\n      </mt-field>\r\n      <mt-field \r\n        type=\"password\"\r\n        label=\"确认密码：\"\r\n        placeholder=\"请再次输入密码\"\r\n        :attr=\"{maxlength:20,autocomplete:'off'}\"\r\n        v-model=\"conpassword\"\r\n        :state=\"conpwdStatus\"\r\n        @blur.native.capture=\"handle3\"> \r\n      </mt-field>\r\n      <!-- 账户信息结束 -->\r\n      <!-- 登录注册按钮开始 -->\r\n      <div  class=\"L_button\">\r\n       <mt-button @click=\"handle\" class=\"register-button\">免费注册</mt-button>\r\n       <mt-button  class=\"login-button\" @click=\"login()\"> 已有账号,返回登录</mt-button>\r\n      </div>\r\n      <!-- 登录注册按钮结束 -->\r\n    </div>    \r\n  </div>\r\n</template>\r\n<style scoped>\r\n  /* 顶部文字样式 */\r\n  .header{\r\n        background-color:#FFF;\r\n        font:italic 20px \"simhei\";\r\n        color: #000;\r\n  }\r\n  /* 注册登录按钮样式 */\r\n    .register-button,.login-button{\r\n      display:block;\r\n      margin:17px auto;\r\n      width: 240px;\r\n      height: 40px;\r\n      font:16px \"Microsoft yahei\";\r\n      color: #FFF;\r\n      border-radius: 20px;\r\n      background-color: rgba(0, 183, 255, 0.815);\r\n    }\r\n    .login-button{\r\n        background-color: rgb(7,193,96);\r\n        margin-top: 17px;\r\n    }\r\n    /* a标签去除下划线 */\r\n    a{\r\n      text-decoration: none;\r\n    }\r\n    /* 图片居中 */\r\n    img{\r\n      display:block;\r\n      margin:auto;\r\n    }\r\n</style>\r\n<script>\r\n  export default {\r\n    data(){\r\n      return{\r\n        username:'',\r\n        password:'',\r\n        conpassword:'',\r\n        NameStatus:'',\r\n        pwdStatus:'',\r\n        conpwdStatus:''\r\n      }\r\n    },\r\n    methods:{\r\n      //返回登录跳转\r\n      login(){\r\n       this.$router.push(\"/login\")\r\n      },\r\n      handle1(){\r\n        // 1.验证用户名是否正确\r\n        let userReg=/^[0-9a-zA-Z_@\\.]{6,20}$/;\r\n        if(userReg.test(this.username)){\r\n          // console.log(\"用户名正确\")\r\n          this.NameStatus=\"success\";\r\n          return true\r\n        }else{\r\n          // console.log(\"用户名非法\");\r\n          this.NameStatus=\"error\";\r\n          // this.$toast(\"用户名非法\")\r\n          this.$toast({\r\n            message:\"用户名不正确\",\r\n            position:\"top\",\r\n          });\r\n          return false\r\n        }\r\n      },\r\n      handle2(){\r\n        // 2.验证密码是否正确\r\n        let upwd=/^[0-9a-zA-Z_\\.]{6,20}$/\r\n        if(upwd.test(this.password)){\r\n          // console.log(\"用户名正确\")\r\n          this.pwdStatus=\"success\";\r\n          return true\r\n        }else{\r\n          this.$toast({\r\n            message:\"密码不正确\",\r\n            position:\"bottom\"\r\n          });\r\n          this.pwdStatus=\"error\";\r\n          return false\r\n        }\r\n      },\r\n      handle3(){\r\n        // 3.验证确认密码是否正确\r\n        if(this.conpassword!=\"\"){\r\n          if(this.password!=this.conpassword){\r\n            this.$toast({\r\n              message:\"两次密码不一致\",\r\n              position:\"bottom\",\r\n            });\r\n            return false\r\n            this.conpwdStatus=\"error\";\r\n\r\n          }else{\r\n            this.conpwdStatus=\"success\";\r\n            return true\r\n          }\r\n        }\r\n      },\r\n      handle(){\r\n        if(this.handle1() && this.handle2() && this.handle3()){\r\n          // 将获取到的信息提交到web服务器\r\n          this.axios.post('/register','username=' + this.username + '&password=' + this.password).then(res=>{\r\n            // console.log(res.data)\r\n            // 用户注册成功，跳转到登录页面\r\n            if(res.data.code==1){\r\n              // this.$toast({message:'注册成功'})\r\n              this.$router.push('/')\r\n            }else{\r\n              // this.$toast({message:'用户名已存在'})\r\n              this.$messagebox('注册提示','用户名已被占用')\r\n            }\r\n          });\r\n        }\r\n      }\r\n    }\r\n  }\r\n</script>"],"sourceRoot":"src/views"}]}