,QAAA,CAAA,aAAA,CAAA,UAAA,EAAA,SAAA,GAAA,KAAA;AACA,KANA;AAOA;AACA,IAAA,SARA,uBAQA;AACA,WAAA,QAAA,GAAA,KAAA,CADA,CAEA;;AACA,MAAA,QAAA,CAAA,aAAA,CAAA,UAAA,EAAA,SAAA,GAAA,IAAA;AACA;AAZA,GARA;AAsBA,EAAA,OAtBA,qBAsBA;AACA;AACA;AACA;AACA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,KAAA,OAAA,CAAA,KAAA,CAAA,IAAA,EALA,CAMA;;AACA,SAAA,KAAA,CAAA,GAAA,CAAA,iBAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,IAAA,GAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,MAAA,IAAA,CACA,IADA,CACA,IADA,GACA,IADA;AAEA,KALA;AAMA;AAnCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"details\">\r\n    <header>\r\n      <!-- 顶部导航栏 -->\r\n      <mt-header fixed>\r\n        <!-- 左边的返回键 -->\r\n        <div slot='left'>\r\n          <router-link to='/'>\r\n            <mt-button icon='back'></mt-button>\r\n          </router-link>\r\n        </div>\r\n        <!-- 右边链接键 -->\r\n        <div slot=\"right\">\r\n          <!-- 分享链接 -->\r\n          <router-link to=\"/\">\r\n            <img src=\"../../public/img/details-image/链接.png\" alt=\"\" class=\"img-header\">\r\n          </router-link>\r\n          <!-- 购物车链接 -->\r\n          <router-link to=\"/shopcart\">\r\n            <img src=\"../../public/img/details-image/购物车.png\" class=\"img-header\">\r\n          </router-link>\r\n          <!-- 更多链接 -->\r\n          <router-link to=\"/\" class=\"img-header\">\r\n          <!-- 更多按钮 -->\r\n            <mt-button icon=\"more\" ></mt-button>\r\n          </router-link>\r\n        </div>\r\n      </mt-header>\r\n    </header>\r\n    <div class=\"swipe\">\r\n      <mt-swipe  :auto=\"50000\">\r\n        <mt-swipe-item><img src=\"../../public/img/detail/detail_img1.jpg\" alt=\"\"></mt-swipe-item>\r\n      </mt-swipe>\r\n    </div>\r\n    <!-- 第二栏开始 -->\r\n    <!-- 第二栏设置唯一父元素    -- money-->\r\n    <div class=\"money\">\r\n      <!-- 第二栏商品价格 -->\r\n      <div class=\"price\">{{info.price}}</div>\r\n      <!-- 第二栏商品标题 -->\r\n      <span class=\"cotton\">{{info.title}}</span>\r\n      <!-- 第二栏商品详情介绍 -->\r\n      <div class=\"recommend\">{{info.subtitle}}</div>\r\n      <!-- 第二栏末尾导航栏 -->\r\n      <div class=\"love\">\r\n      <!-- 第二栏末尾推荐导航标志 -->\r\n        <router-link to=\"/\" class=\"love-router\">\r\n          <img src=\"../../public/img/details-image/xin.png\" alt=\"\">\r\n          推荐\r\n        </router-link>\r\n        <!-- 第二栏末尾帮我选导航标志 -->\r\n        <router-link to=\"/\" class=\"love-router\">\r\n          <img src=\"../../public/img/details-image/人员.png\" alt=\"\">\r\n          帮我选\r\n        </router-link>\r\n        <!-- 第二栏末尾分享链接标志 -->\r\n        <router-link to=\"/\" class=\"love-router\">\r\n          <img src=\"../../public/img/details-image/链接1.png\" alt=\"\">\r\n          分享\r\n        </router-link>\r\n      </div>\r\n    </div>\r\n    <!-- 第二栏结束 -->\r\n    <!-- 第三栏开始 -->\r\n    <!-- 设置唯一父元素 --  three -->\r\n    <div class=\"three\">\r\n      <!-- 使用表格样式，避免过多重复的div和p标签的使用 -->\r\n      <table>\r\n        <tr>\r\n          <td>选择</td>\r\n          <td>\r\n            <!-- 颜色分类图片链接 -->\r\n            <router-link to=\"/shopcart\" class=\"three-router\">\r\n              <span>尺寸 / 颜色分类</span>\r\n              <!-- 四张小图片的样式   - v-for遍历数据库找数据 -->\r\n              <div><img src=\"../../public/img/detail/detail_img1.jpg\" class=\"three-img\">\r\n              <!-- 末尾尖括号图片 -->\r\n              <img src=\"../../public/img/details-image/大于号.png\" class=\"three-image\">\r\n              </div>\r\n            </router-link>\r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td>发货</td>\r\n          <td>\r\n            <!-- 快递链接导航 -->\r\n            <router-link to=\"/site\" class=\"three-router\">\r\n              <span>江苏 | 快递：免运费\r\n                <!-- 末尾尖括号导航栏-->\r\n                <img src=\"../../public/img/details-image/大于号.png\" class=\"three-image\">\r\n              </span>\r\n              <div class=\"three-site\">\r\n                配送至：\r\n                <span></span>\r\n              </div>\r\n            </router-link>\r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td>保障</td>\r\n          <td>\r\n            <!-- 保障栏链接导航 -->\r\n            <router-link to=\"/\" class=\"three-router\">\r\n              假一赔四 · 退货运费险 · 极速退款\r\n              <!-- 末尾尖括号导航 -->\r\n              <img src=\"../../public/img/details-image/大于号.png\" class=\"three-image\">\r\n            </router-link> \r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td>权益</td>\r\n          <td>\r\n            <!-- 芝麻分导航栏 -->\r\n            <router-link to=\"/\" class=\"three-router\">\r\n              凭芝麻分先用后付，收货满意再付   \r\n              <span class=\"three-position\">开通</span> \r\n              <!-- 末尾尖括号导航 -->\r\n              <img src=\"../../public/img/details-image/大于号.png\" class=\"three-image\">\r\n            </router-link>\r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td>参数</td>\r\n          <td>\r\n            <!-- 参数导航栏链接 -->\r\n            <router-link to=\"/\" class=\"three-router\">\r\n              品牌 产地 . . .\r\n              <!-- 末尾尖括号导航 -->\r\n              <img src=\"../../public/img/details-image/大于号.png\" class=\"three-image\">\r\n            </router-link>\r\n          </td>\r\n        </tr>\r\n      </table>\r\n    </div>\r\n    <!-- 第三栏结束 -->\r\n    <!-- 第四栏开始 -->\r\n    <!-- 给第四栏创建一个唯一父元素 -->\r\n    <div class=\"four\">\r\n      <div class=\"four-son\">\r\n        <div>\r\n          <!-- 第一层标签（宝贝介绍） -->\r\n          <div>\r\n            <span class=\"four-baby\">宝贝评价()</span>\r\n            <router-link to=\"/\" class=\"four-all\">查看全部 > </router-link>\r\n          </div>\r\n          <!-- 各种各样的评价，以及汇总 -->\r\n          <div>\r\n            <button class=\"four-button\">性价比高()</button>\r\n            <button class=\"four-button\">质量好()</button>\r\n            <button class=\"four-button\">很舒适()</button>\r\n          \r\n            <div>\r\n              <!-- 某某用户的评价，最近几天的 -->\r\n              <img src=\"../../public/img/details-image/头像 男孩.png\" alt=\"\">\r\n              <span>用户名</span>\r\n              <div class=\"four-product\">暗室逢灯卡时间划分空间按时付款就好撒空间发件爱神的箭返回尽快发货</div>\r\n              <!-- 用户上传的照片 -->\r\n              <div class=\"four-img-fu\">\r\n                <img src=\"../../public/img/detail/detail_img1.jpg\" class=\"four-img\">\r\n              </div>\r\n            </div>\r\n          \r\n          </div>\r\n        </div>\r\n        <hr>\r\n        <div>\r\n          <!-- 询问买过的人，以及店家详细信息 -->\r\n          <div>\r\n            <span class=\"four-baby\">问大家()</span>\r\n            <router-link to=\"/\" class=\"four-all\">查看全部 ></router-link>\r\n          </div>\r\n          <!-- 问题提问区 -->\r\n          <div class=\"four-last-fu\">\r\n            <span class=\"four-last\">问</span>\r\n            <span>着安徽科技护肤的撒娇和飞机撒副科级</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- 第四栏结束 -->\r\n    <!-- 第五栏开始 -->\r\n    <p class=\"baby-product\">宝贝详情</p>\r\n    <!-- 公益公告 -->\r\n    <div class=\"good-baby\">\r\n      <img src=\"../../public/img/details-image/公益.png\" class=\"good-baby-img\">\r\n      <div>\r\n        <div>公益宝贝</div>\r\n        <div class=\"good-baby-text\">每笔成交将为 <span class=\"good-baby-span\">免费午餐基金</span> 捐赠 <span class=\"good-baby-span\">0.02元</span> ， 已累计捐赠 <span class=\"good-baby-span\">98658</span> 笔。</div>\r\n      </div>\r\n    </div>\r\n    <div class=\"products\">\r\n      <img src=\"../../public/img/detail/detail_img11.jpg\" class=\"products-img\">\r\n      <!-- 底部详情页信息 -->\r\n      <div class=\"products-table\">\r\n        <table>\r\n          <tr>\r\n            <td class=\"products-td\">价格说明</td>\r\n          </tr>\r\n          <tr>\r\n            <td>\r\n              <span class=\"products-spot\">·</span>\r\n              <span class=\"products-text\">划线价格</span>\r\n            </td>\r\n          </tr>\r\n          <tr>\r\n            <td>\r\n              商品的专柜价、吊牌价、正品零售价、厂商指导价或该商品的曾经展示过的销售价等，<span class=\"products-text\">并非原价</span> ,仅供参考\r\n            </td>\r\n          </tr>\r\n          <tr>\r\n            <td>\r\n              <span class=\"products-spot\">·</span>\r\n              <span class=\"products-text\">未划线价格</span>\r\n            </td>\r\n          </tr>\r\n          <tr>\r\n            <td>商品的 <span class=\"products-text\">实时标价</span> ,不因表述的差异改变性质。具体成交价格根据商品参加活动，或会员使用优惠券、积分等发生变化，最终以定单结算页价格为准</td>\r\n          </tr>\r\n          <tr>\r\n            <td>\r\n              <span class=\"products-spot\">·</span>\r\n              <span>商家详情页 (含主图) 以图片或文字形式标注的一口价、促销价、优惠价等价格可能是在使用优惠券、慢件或特定优惠活动和时段等情形下的价格，具体请以结算页面的标价、优惠条件或活动规则为准。</span>\r\n            </td>\r\n          </tr>\r\n          <tr>\r\n            <td>\r\n              <span class=\"products-spot\">·</span>\r\n              <span>此说明仅当出现价格比较时有效，具体请参见《淘宝价格发布规范》。若商家单独对划线价格进行说明的，以商家的表述为准。</span>\r\n            </td>\r\n          </tr>\r\n        </table>\r\n      </div>\r\n    </div>\r\n    <!-- 底部固定导航栏 --店铺--客服---收藏---加入购物车---立即购买 -->\r\n    <div class=\"last-down\">\r\n        <ul class=\"last-ul\">\r\n          <li>\r\n            <ul>\r\n              <!-- 店铺连接   -->\r\n              <router-link to=\"/\">\r\n              <!-- 店铺下的图片以及文字的内容 -->\r\n                <li><img src=\"../../public/img/details-image/店铺.png\" class=\"last-img\"></li>\r\n                <li>店铺</li>\r\n              </router-link>\r\n            </ul>\r\n          </li>\r\n          <li>\r\n            <ul>\r\n              <!-- 客服导航栏的内容 -->\r\n              <router-link to=\"/\">             \r\n                <li><img src=\"../../public/img/details-image/客服.png\" class=\"last-img\"></li>\r\n                <li>客服</li>\r\n              </router-link>\r\n            </ul>\r\n          </li>\r\n          <li>\r\n            <ul>\r\n              <div class=\"last-ul-div\">\r\n                <!-- 收藏栏的内容 -->\r\n                <li>\r\n                  <!-- 点击函数--lastImg--用v-if--来实现图片切换效果 -->\r\n                  <img src=\"../../public/img/details-image/收藏2.png\"   c