A,CAAA,EAAA,EAAA,KAAA,GAAA,CAAA,EAAA;AACA,iBAAA,KAAA,CAAA,EAAA,EAAA,KAAA,GAAA,CAAA;AACA,WARA,CASA;;AACA,SAVA,MAUA,IAAA,CAAA,CAAA,MAAA,CAAA,QAAA,IAAA,OAAA,EAAA,CACA;AACA,KArBA;AAsBA,IAAA,KAtBA,mBAsBA;AACA,UAAA,MAAA,GAAA,KAAA,KAAA,CAAA,KAAA,CAAA,UAAA,IAAA,EAAA;AACA;AACA;AACA,eAAA,IAAA,CAAA,UAAA,IAAA,IAAA;AAEA,OALA,CAAA;AAMA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,MAAA,EAPA,CAQA;;AACA,WAAA,QAAA,GAAA,MAAA;AACA,KAhCA;AAiCA;AACA,IAAA,MAlCA,oBAkCA;AACA;AACA,UAAA,KAAA,QAAA,IAAA,KAAA,EAAA;AAAA,mDACA,KAAA,KADA;AAAA;;AAAA;AACA,8DAAA;AAAA,gBAAA,GAAA;AACA,YAAA,GAAA,CAAA,UAAA,GAAA,IAAA;AACA;AAHA;AAAA;AAAA;AAAA;AAAA;AAIA,OAJA,MAIA;AAAA,oDACA,KAAA,KADA;AAAA;;AAAA;AACA,iEAAA;AAAA,gBAAA,IAAA;AACA,YAAA,IAAA,CAAA,UAAA,GAAA,KAAA;AACA;AAHA;AAAA;AAAA;AAAA;AAAA;AAIA;AACA;AA7CA,GArBA;AAqEA,EAAA,QAAA,EAAA;AACA,IAAA,KADA,mBACA;AACA,UAAA,KAAA,GAAA,CAAA;;AADA,kDAEA,KAAA,KAFA;AAAA;;AAAA;AAEA,+DAAA;AAAA,cAAA,CAAA;;AACA,cAAA,CAAA,CAAA,UAAA,EAAA;AACA,YAAA,KAAA,IAAA,CAAA,CAAA,KAAA,GAAA,CAAA,CAAA,KAAA;AACA;AACA,SANA,CAOA;;AAPA;AAAA;AAAA;AAAA;AAAA;;AAQA,aAAA,KAAA;AACA,KAVA;AAWA,IAAA,MAXA,oBAWA;AACA,UAAA,MAAA,GAAA,CAAA;;AADA,kDAEA,KAAA,KAFA;AAAA;;AAAA;AAEA,+DAAA;AAAA,cAAA,CAAA;AACA,UAAA,MAAA,IAAA,CAAA,CAAA,KAAA;AACA;AAJA;AAAA;AAAA;AAAA;AAAA;;AAKA,aAAA,MAAA;AACA;AAjBA,GArEA;AAwFA,EAAA,KAAA,EAAA;AACA,IAAA,MADA,kBACA,KADA,EACA;AACA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,MAAA;;AACA,UAAA,KAAA,IAAA,OAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,GAAA,EAAA,KAAA,CAAA,UAAA,CAAA,EAAA,CAAA,CAAA;AACA,aAAA,MAAA,GAAA,OAAA;AACA;;AAAA,UAAA,KAAA,IAAA,IAAA,EAAA;AACA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,KAAA,EAAA,KAAA,CAAA,UAAA,CAAA,EAAA,CAAA,CAAA;AACA,aAAA,MAAA,GAAA,IAAA;AACA;;AAAA,UAAA,KAAA,IAAA,SAAA,EAAA;AACA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,UAAA,EAAA,KAAA,CAAA,UAAA,CAAA,EAAA,CAAA,CAAA;AACA,aAAA,MAAA,GAAA,SAAA;AACA;;AAAA,UAAA,KAAA,IAAA,MAAA,EAAA;AACA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,OAAA,EAAA,KAAA,CAAA,UAAA,CAAA,EAAA,CAAA,CAAA;AACA,aAAA,MAAA,GAAA,MAAA;AACA;AACA;AApBA;AAxFA,CAAA","sourcesContent":["<template>\r\n  <div class=\"cart\">\r\n    <div class=\"nav\">\r\n      <div>\r\n        <span>购物车</span>\r\n        <span>管理</span> \r\n      </div>\r\n      <p>共{{counts}}件商品</p>\r\n    </div>\r\n    <ul class=\"product\" @click=\"handle\">\r\n      <li v-for=\"(item,index) of shops\" :key=\"index\" >\r\n        <input type=\"checkbox\" v-model=\"item.is_checked\" @click=\"check(item)\">\r\n        <!-- <span>{{item.is_checked}}</span> -->\r\n        <img :src=\"item.pic\" alt=\"\">\r\n        <div class=\"pro_details\">\r\n          <p>{{item.title}} <button :data-i=\"index\">×</button></p>\r\n          <p>{{item.details}}</p>\r\n          <p>{{item.price}}</p>\r\n        </div>\r\n        <button :data-i=\"index\" :data-n=\"-1\">-</button>\r\n        <span>{{item.count}}</span>\r\n        <button :data-i=\"index\" :data-n=\"+1\">+</button>\r\n      </li>\r\n    </ul>\r\n      <div class=\"all\">\r\n        <span><input type=\"checkbox\" v-model=\"is_check\" @click=\"cheAll\">全选</span>\r\n        <span>总价：￥{{total.toFixed(2)}}</span> \r\n      </div>\r\n      <my-footer></my-footer>\r\n  </div>  \r\n</template>\r\n<style scoped>\r\n\r\n  .cart button{\r\n    background: #fff;\r\n    border: 1px solid #e8e8e8;\r\n  }\r\n  /*设置底部选项卡选中时的颜色 */\r\n  a.is-selected{\r\n    color: #d4237a !important;\r\n  }\r\n  /*顶部导航栏容器 */\r\n  .nav{\r\n    padding: 10px;\r\n  }\r\n  .nav>div{\r\n    display: flex;\r\n    justify-content: space-between;\r\n  }\r\n  .nav>div>span:first-child{\r\n    font-size: 24px;\r\n    font-weight: bold;\r\n  }\r\n  /*商品列表容器*/\r\n  .product{\r\n    margin-bottom: 55px;\r\n    \r\n  }\r\n  .product li{\r\n    background-color: #fff;\r\n    border-radius: 5px;\r\n    box-shadow: 0 0 2px 2px #eee;\r\n    height: 100px;\r\n    margin: 10px;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    padding: 10px;\r\n  }\r\n  .product img{\r\n    width: 80px;\r\n    margin-left: 10px;\r\n  }\r\n  /*价格及信息容器*/\r\n  .pro_details{\r\n    margin:0 10px\r\n  }\r\n  .pro_details>p:first-child{\r\n    font-size: 18px;\r\n    margin-bottom: 25px;\r\n    font-weight: bold;\r\n  }\r\n  .pro_details>p:last-child{\r\n    font-size: 14px;\r\n    margin-bottom: 10px;\r\n  }\r\n  .product button{\r\n    outline: none;\r\n  }\r\n  .product>li>button:first-child{\r\n    padding-right: 5px;\r\n    border: 1px solid #666;\r\n  }\r\n  /*底部总价容器*/\r\n  .all{\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    position: fixed;\r\n    left: 0;\r\n    right: 0;\r\n    bottom: 55px;\r\n    border-top: 1px solid #eee;\r\n    background-color: #fff;\r\n    padding-left: 10px;\r\n    padding-right: 10px;\r\n    height: 50px;\r\n  }\r\n</style>\r\n<script>\r\nimport Search from \"../components/Search\";\r\nimport MyFooter from \"../components/MyFooter\";\r\n\r\n  \r\nexport default {\r\n  components:{\r\n    Search,MyFooter\r\n  },\r\n  data(){\r\n    return{\r\n      // 底部导航栏变量\r\n      tabbar:'shop',\r\n      // 用于存储商品信息\r\n      shops:[],\r\n      // 用于存储商品是否选中\r\n      is_check:false\r\n    }\r\n  },\r\n  mounted(){\r\n    console.log(this.tabbar)\r\n    this.axios.get('/cart').then(res=>{\r\n      this.shops = res.data.results\r\n      // console.log(this.shops[0].is_checked)\r\n    })\r\n  },\r\n  methods:{\r\n    // 商品处理事件，包括是否选中，加减商品，删除商品\r\n    handle(e){\r\n      // 当点击的标签是<button>且内容是×就删除当前点击的商品\r\n      if(e.target.nodeName == \"BUTTON\" && e.target.innerHTML == '×'){\r\n        let i = e.target.dataset.i;\r\n        this.shops.splice(i,1)\r\n      }\r\n      // 否则点击的标签是<button>时\r\n      else if(e.target.nodeName == \"BUTTON\"){\r\n        // 得到用自定义属性保存的下标index\r\n        let i = e.target.dataset.i;\r\n        // 当前点击的数量加或减\r\n        this.shops[i].count+=parseInt(e.target.dataset.n) ;\r\n        // 当点击减到1时，始终为1\r\n        if(this.shops[i].count<1){\r\n          this.shops[i].count = 1;\r\n        }\r\n        // 如果点击的是input时,当点击取消选中一个时就取消全选\r\n      }else if(e.target.nodeName == \"INPUT\"){\r\n        }\r\n    },\r\n    check(){\r\n      let select = this.shops.every(item=>{\r\n        // console.log(item.is_checked)\r\n        // console.log(item.is_checked)\r\n          return item.is_checked == true;\r\n          \r\n        })\r\n        console.log(select.length)\r\n        // select.length == this.shops.length ? this.is_check = true : this.is_check = false\r\n        this.is_check = select\r\n    },\r\n    // 当点击全选时，商品全部选中\r\n    cheAll(){\r\n      // 如果点击当前的按钮为false，点击后则为true，就让所有按钮变为true，即可全部选中\r\n      if(this.is_check==false){\r\n        for(let key of this.shops){\r\n          key.is_checked = true\r\n        }\r\n      }else{\r\n        for(let key of this.shops){\r\n          key.is_checked = false\r\n        }\r\n      }\r\n    }\r\n  },\r\n  \r\n  computed:{\r\n    total(){\r\n      let total = 0;\r\n      for(let p of this.shops){\r\n        if(p.is_checked){\r\n          total += p.price*p.count\r\n        }\r\n      }\r\n      // console.log(this.shops)\r\n      return total;\r\n    },\r\n    counts(){\r\n      let counts = 0;\r\n      for(let p of this.shops){\r\n        counts +=p.count\r\n      }\r\n      return counts;\r\n    }\r\n  },\r\n  watch:{\r\n    tabbar(value){\r\n      // 跳转到主页\r\n      console.log(this.tabbar)\r\n      if(value=='index'){\r\n        this.$router.push('/').catch(e=>{})\r\n        this.tabbar = 'index'\r\n      }if(value=='me'){\r\n      // 跳转到我的页面\r\n        this.$router.push('/me').catch(e=>{})\r\n        this.tabbar = 'me'\r\n      }if(value=='message'){\r\n        // 跳转到消息页面\r\n        this.$router.push('/message').catch(e=>{})\r\n        this.tabbar = 'message'\r\n      }if(value=='shop'){\r\n        // 跳转到购物车\r\n        this.$router.push('/shop').catch(e=>{})\r\n        this.tabbar = 'shop'\r\n      }\r\n    }\r\n  } \r\n}\r\n</script>"],"sourceRoot":"src/views"}]}